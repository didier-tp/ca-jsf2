<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
    
    <navigation-rule>
       <from-view-id>/emprunt.xhtml</from-view-id>
       
       <!-- navigation-case par defaut sans redirect (performant) -->
       <!-- avec redirect , URL actualisée après navigation
            mais double requête envoyée par navigateur et donc
            mauvaise performance et quelquefois besoin de @SessionScoped -->
       <!-- 
		<navigation-case>
			<from-outcome>resEmprunt</from-outcome>
			<to-view-id>/resEmprunt.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	   -->
	</navigation-rule>

</faces-config>
